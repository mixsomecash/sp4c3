    "classnames": "^2.3.1",
    "ethers": "^5.1.0",
    "framer-motion": "^4.1.17",
    "moralis": "^1.3.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-moralis": "^1.3.1",
    "react-router-dom": "^5.2.0",
